{
  "name": "@alorel/rollup-plugin-web-worker",
  "version": "1.0.2",
  "description": "Bundles web workers as chunks",
  "main": "index.cjs.js",
  "module": "index.es.js",
  "repository": "https://github.com/Alorel/rollup-plugin-web-worker.git",
  "types": "index.d.ts",
  "typings": "index.d.ts",
  "author": "Arturas Molcanovas <a.molcanovas@gmail.com>",
  "license": "MIT",
  "scripts": {
    "rollup": "del-cli -f dist && rollup -c --fcjs2015 --fesm2015 --dts",
    "rollup:watch": "del-cli -f dist && rollup -c --fcjs2015 --fesm2015 --watch",
    "release": "semantic-release",
    "doctoc": "doctoc --github README.md",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "yarn run lint --fix",
    "test": "del-cli coverage && cross-env \"TS_NODE_PROJECT=tsconfig.test.json\" nyc mocha",
    "test:watch": "yarn run test --watch",
    "typecheck": "tsc --noEmit --outDir dist",
    "typecheck:watch": "yarn run typecheck --watch"
  },
  "peerDependencies": {
    "rollup": "^2.0.0"
  },
  "devDependencies": {
    "@alorel-personal/conventional-changelog-alorel": "^2.1.3",
    "@alorel/eslint-config-base": "^1.0.18",
    "@alorel/eslint-config-typescript": "^1.0.17",
    "@alorel/rollup-plugin-copy": "^1.0.2",
    "@alorel/rollup-plugin-copy-pkg-json": "^1.0.3",
    "@alorel/rollup-plugin-dts": "^2.0.1",
    "@alorel/rollup-plugin-threaded-terser": "^1.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.1.1",
    "@semantic-release/npm": "^7.0.6",
    "@types/chai": "^4.2.13",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.8",
    "@types/sinon": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^7.0.2",
    "del-cli": "^3.0.1",
    "doctoc": "^1.4.0",
    "eslint": "^7.11.0",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "rollup": "^2.29.0",
    "rollup-plugin-typescript2": "^0.27.3",
    "semantic-release": "^17.1.2",
    "sinon": "^9.2.0",
    "source-map-support": "^0.5.19",
    "terser": "^5.3.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "lazy-get-decorator": "^2.2.0",
    "magic-string": "^0.25.7",
    "tslib": "^2.0.0 || ^1.9.0"
  }
}
